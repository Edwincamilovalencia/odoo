<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Secuencia para el historial de llamadas -->
    <data noupdate="1">
        <record id="seq_retain_call_history" model="ir.sequence">
            <field name="name">Secuencia de Llamadas</field>
            <field name="code">retain.call.sequence</field>
            <field name="prefix">CALL-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>